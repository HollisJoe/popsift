# - Config file for the popsift package
# It defines the following variables
#  POPSIFT_INCLUDE_DIRS - include directories for popsift 
#  POPSIFT_LIBRARIES    - libraries to link against
 
set(POPSIFT_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

if(NOT TARGET popsift) 
  get_filename_component(POPSIFT_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
  include("${POPSIFT_CMAKE_DIR}/popsiftTargets.cmake")
endif()
set(POPSIFT_LIBRARIES popsift)
